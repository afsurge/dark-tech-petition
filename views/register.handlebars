<div id="registerBody">
    <h1>RISE AGAINST THE DARK FORCE</h1>
    <h1>Stand with us to ban the exploitation of social media</h1>
    <img src="#" alt="">
    <form method="POST">
        {{#if error}}
        <h3 id="errorMsg">{{errorMsg}}</h3>
        {{/if}}
        <p>first name</p>
        <input type="text" name="first">
        <p>last name</p>
        <input type="text" name="last">
         <p>email</p>
        <input type="text" name="email">
        <p>password</p>
        <input type="password" name="password">
        <input type="hidden" name="_csrf" value="{{csrfToken}}">
        <br>
        <br>
        <button id="signUp">JOIN US!</button>
    </form>
    <br>
    <h2>Already registered with us? <a href="/login">Log in</a>.</h2>
</div>