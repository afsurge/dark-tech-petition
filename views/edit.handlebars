<div class="pageBody">
    <h2>Edit your profile</h2>
    <form class="otherForms" method="POST">
        {{#rows}}
        <p>first name</p>
        <input type="text" name="first" value="{{first}}">
        <p>last name</p>
        <input type="text" name="last" value="{{last}}">
        <p>email</p>
        <input type="text" name="email" value="{{email}}">
        <p>password</p>
        <input type="password" name="password">
        <p>age (18 and above)</p>
        <input type="text" name="age" value="{{age}}">
        <p>city</p>
        <input type="text" name="city" value="{{city}}">
        <p>homepage</p>
        <input type="text" name="url" value="{{url}}">
        {{/rows}}
        <input type="hidden" name="_csrf" value="{{csrfToken}}">
        <br>
        {{#if error}}
        <h3 class="errorMsg">{{errorMsg}}</h3>
        {{/if}}
        <br>
        {{#if errorAge}}
        <h3 class="errorMsg">{{errorAgeMsg}}</h3>
        {{/if}}
        <br>
        <button class="otherButtons" id="udpateProfile">UPDATE</button>
    </form>
    <a class="allForms" href="/delete"><button class="allButtons">DELETE ACCOUNT</button></a>
</div>
<div class="pageBanner"></div>